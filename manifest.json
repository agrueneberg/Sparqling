{
    "manifest_version": 2,
    "name": "Sparqling",
    "description": "An RDF store in your browser",
    "version": "0.3.1",
    "app": {
        "launch": {
            "local_path": "main.html"
        }
    },
    "icons": {
        "128": "icon_128.png",
        "16": "icon_16.png"
    },
    "intents": {
        "http://mathbiol.org/intents/sparql": {
            "type": ["text/plain"],
            "href": "main.html",
            "disposition": "inline",
            "title": "Query Sparqling"
        }
    }
}
