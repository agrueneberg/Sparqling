{
    "manifest_version": 2,
    "name": "Sparqling",
    "description": "An RDF store in your browser",
    "version": "0.5.0",
    "app": {
        "launch": {
            "local_path": "main.html"
        }
    },
    "icons": {
        "128": "icon_128.png",
        "16": "icon_16.png"
    },
    "permissions": [
        "<all_urls>"
    ]
}